@model IEnumerable<TheatreCMS3.Areas.Blog.Models.BlogPost>
<script src="https://kit.fontawesome.com/634caf7d3f.js" crossorigin="anonymous"></script>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2 class="blogpost-createandedit-header">Blog Posts</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
@foreach (var item in Model)
{
    <div class="card mb-3 blogpost-index-container" style="max-width: 540px;">
        <div class="row g-0">
            <div class="col-md-4">
                <img src="data:image/png;base64,"
                     alt="placeholder image" class="img-fluid rounded-start" />
            </div>
            <div class="col-md-8">
                <div class="card-body blogpost-index-box">
                    <h4 class="card-text blogpost-index-author-posted">
                        @Html.DisplayFor(modelItem => item.Author)
                        <br />
                        @Html.DisplayFor(modelItem => item.Posted)
                    </h4>
                    <h5 class="card-title blogpost-index-title">
                        @Html.DisplayFor(modelItem => item.Title)
                    </h5>
                    <p class="card-text blogpost-index-content">
                        @Html.DisplayFor(modelItem => item.Content)
                    </p>
                    <div>
                        <a href='@Url.Action("Delete", "BlogPost", new { id = item.BlogPostId })' class='btn blogpost-index-delete blogpost-index-buttons'><i class="fa fa-trash"></i> Delete</a>
                    </div>
                    <div>
                        <a href='@Url.Action("Details", "BlogPost", new { id = item.BlogPostId })' class='btn blogpost-index-details blogpost-index-buttons'><i class="fa fa-info-circle"></i> Details</a>
                    </div>
                    <div>
                        <a href='@Url.Action("Edit", "BlogPost", new { id = item.BlogPostId })' class='btn blogpost-index-edit blogpost-index-buttons'><i class="fa fa-edit"></i> Edit</a>
                    </div>
                </div>
            </div>
        </div>
    </div>

}


